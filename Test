Page<AlertManagement> alertEntities = alertManagementRepository.findAll(pageable);

// Suppose you're mapping entity to response
Page<AlertManagementResponse> alertPage = alertEntities.map(alert -> AlertManagementResponse.builder()
        .alertDescription(alert.getDescription())
        .alertIdentifier(alert.getId())
        .isViewed(alert.isViewed())
        .createdAt(alert.getCreatedAt())
        .build());

List<AlertManagementResponse> alertList = alertPage.getContent();
